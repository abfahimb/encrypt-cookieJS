{
    "name": "encrypt-cookiejs",
    "version": "1.0.2",
    "description": "encrypt-cookiejs is a lightweight JavaScript package designed to securely manage data in browser cookies using advanced encryption techniques. This package ensures that sensitive data stored in cookies is encrypted, providing a higher level of security for client-side storage",
    "main": "dist/encrypt-cookie.js",
    "types": "dist/encrypt-cookie.d.ts",
    "browser": "dist/encrypt-cookie.js",
    "unpkg": "dist/encrypt-cookie.min.js",
    "jsdelivr": "dist/encrypt-cookie.min.js",
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "keywords": [
        "encrypt-cookiejs",
        "cookie",
        "cookies",
        "browser",
        "commonjs",
        "client",
        "browserify",
        "encryption"
    ],
    "homepage": "https://github.com/abfahimb/encrypt-cookieJS#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/abfahimb/encrypt-cookieJS.git"
    },
    "author": "Abdullah Al Fahim",
    "license": "MIT",
    "scripts": {
        "build": "tsc && terser dist/encrypt-cookie.js -o dist/encrypt-cookie.min.js --compress --mangle --comments '/^!/'",
        "prepare": "npm run build"
    },
    "devDependencies": {
        "terser": "^5.0.0",
        "typescript": "^4.9.5"
    },
    "dependencies": {}
}
